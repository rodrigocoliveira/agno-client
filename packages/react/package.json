{
  "name": "@rodrigocoliveira/agno-react",
  "version": "1.0.1",
  "description": "React hooks and pre-built UI components for Agno client",
  "author": "rodrigocoliveira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigocoliveira/agno-client.git",
    "directory": "packages/react"
  },
  "keywords": [
    "agno",
    "agent",
    "ai",
    "react",
    "hooks",
    "hitl",
    "tool-execution",
    "streaming",
    "chat-ui",
    "components"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@rodrigocoliveira/agno-client": "workspace:*",
    "@rodrigocoliveira/agno-types": "workspace:*"
  },
  "peerDependencies": {
